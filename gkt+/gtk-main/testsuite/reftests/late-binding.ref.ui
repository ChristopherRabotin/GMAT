<interface>
  <object class="GtkWindow" id="window">
    <child>
      <object class="GtkLabel">
        <property name="xalign">0.0</property>
        <binding name="label">
          <lookup name="label">label2</lookup>
        </binding>
        <style>
          <class name="bold" />
        </style>
      </object>
    </child>
  </object>
  <object class="GtkLabel" id="label2">
    <property name="label">Bla</property>
  </object>
</interface>
