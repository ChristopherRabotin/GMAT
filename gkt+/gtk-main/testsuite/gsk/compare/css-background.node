transform {
  transform: scale(16,16);
  child: container {

    clip {
      clip: 0 0 30 30;
      child: container {
        rounded-clip {
          clip: 5 5 50 50 / 5;
          child: color {
            color: rgba(255, 0, 0, 255);
            bounds: 5 5 50 50;
          }
        }
        border {
          outline: 5  5 50 50 / 5;
          widths: 3;
          colors: yellow;
        }
      }
    }

