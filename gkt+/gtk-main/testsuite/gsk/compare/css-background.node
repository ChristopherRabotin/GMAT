        color {
          color: black;
          bounds: 7 5 5 1;
        }

        color {
          color: black;
          bounds: 6 6 2 1;
        }

        color {
          color: black;
          bounds: 5 7 2 1;
        }

        color {
          color: black;
          bounds: 5 8 1 3;
        }

        color {
          color: black;
          bounds: 8 8 1 3;
        }

        color {
          color: black;
          bounds: 8 8 3 1;
        }
      }
    }
  }
