          -I../gui/include \
          -I../gui/include/bitmaps \
          -I../gui/app \
          -I../gui/view \
          -I../gui/output \
          -I../gui/foundation \
          -I../gui/plot \
          -I../gui/mission



clean : archclean

archclean :
	rm -rf */*.o
	rm -rf $(TARGETS)
	rm -rf */*~
	rm -rf core
	rm -rf */core
	rm -rf bin/*
	rm -rf obj/*
	rm -rf $(WXTARGET)
	rm -rf $(CONSOLETARGET)
	rm -rf ../matlab/gmat_mex/src/*.o

