# This file now requires the file BuildEnv.mk in the top level folder.

include ../BuildEnv.mk

WXTARGET = lib/libGmatGui.a

CONSOLETARGET = lib/libGmatConsole.a

ifeq ($(CONSOLE_APP),1)
TARGET = $(CONSOLETARGET)
# Console (no wx code) versions of these objects
BRIDGEOBJECTS = executive/MessageInterfaceConsole.o \
					executive/PlotInterfaceConsole.o \
					interpreter/GuiInterpreterConsole.o
else
TARGET = $(WXTARGET)
# GUI versions of these objects
# Note: ExternalOptimizer also uses some wx code, so it is included here

ifeq ($(USE_MATLAB),1)
BRIDGEOBJECTS = executive/MessageInterface.o \
                executive/PlotInterface.o \
                interpreter/GuiInterpreter.o \
                solver/ExternalOptimizer.o \
                solver/FminconOptimizer.o       
else
BRIDGEOBJECTS = executive/MessageInterface.o \
                executive/PlotInterface.o \
                interpreter/GuiInterpreter.o
endif
endif

all: $(TARGET)
gui: $(WXTARGET)
console: $(CONSOLETARGET)

HEADERS = -Iinclude \
          -Ifoundation \
          -Iexecutive \
          -Iattitude \
          -Ispacecraft \
          -Iforcemodel \
          -Ipropagator \
          -Isubscriber \
