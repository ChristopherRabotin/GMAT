GMAT deltaV.Element1 = 0;
GMAT deltaV.Element2 = 0;
GMAT deltaV.Element3 = 0;
GMAT deltaV.DecrementMass = false;
GMAT deltaV.Isp = 300;
GMAT deltaV.GravitationalAccel = 9.810000000000001;

