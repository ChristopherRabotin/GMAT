        <listitem>
          <para>Propagators</para>

          <itemizedlist>
            <listitem>
              <para>LunarSB</para>

              <itemizedlist>
                <listitem>
                  <para>Max Step Size = 45000</para>
                </listitem>

                <listitem>
                  <para>Use Point Masses for Sun, Luna, and Earth</para>
                </listitem>

                <listitem>
                  <para>No primary bodies, gravity models, atmosphere, or
                  magnetic field models</para>
                </listitem>
              </itemizedlist>

              <para>Script Syntax:</para>

              <programlisting><code>Create ForceModel LunarSB_ForceModel;
GMAT LunarSB_ForceModel.CentralBody = Earth;
GMAT LunarSB_ForceModel.PointMasses = {Earth, Sun, Luna};
GMAT LunarSB_ForceModel.Drag = None;
GMAT LunarSB_ForceModel.SRP = Off;
GMAT LunarSB_ForceModel.ErrorControl = RSSStep;

Create Propagator LunarSB;
GMAT LunarSB.FM = LunarSB_ForceModel;
GMAT LunarSB.Type = RungeKutta89;
GMAT LunarSB.InitialStepSize = 60;
GMAT LunarSB.Accuracy = 9.999999999999999e-012;
GMAT LunarSB.MinStep = 0.001;
GMAT LunarSB.MaxStep = 45000;
GMAT LunarSB.MaxStepAttempts = 50;</code></programlisting>

              <screenshot>
                <info>
