                  <para>VX = -0.23245320142355028;</para>
                </listitem>

                <listitem>
                  <para>VY = -0.44627539677580192;</para>
                </listitem>

                <listitem>
                  <para>VZ = 0.085612056628771024;</para>
                </listitem>

                <listitem>
                  <para>DryMass = 1000;</para>
                </listitem>

                <listitem>
                  <para>Cd = 2.2000000000000002;</para>
                </listitem>

                <listitem>
                  <para>Cr = 1.7;</para>
                </listitem>

                <listitem>
                  <para>DragArea = 15;</para>
                </listitem>

                <listitem>
                  <para>SRPArea = 1;</para>
                </listitem>
              </itemizedlist>
            </listitem>
          </itemizedlist>
        </listitem>

        <listitem>
          <para>Find:</para>

