  <refnamediv>
    <refname>ResourceName</refname>

    <refpurpose>A one-sentence description of the resource</refpurpose>
  </refnamediv>

  <refsection>
    <title>Description</title>

    <para>The text description of the resource. Use special formatting for
    <guilabel>command names</guilabel> and <guilabel>field
    names</guilabel>.</para>

    <para><phrase role="ref_seealso">See Also</phrase>: <xref
    linkend="LinkId"/>, <xref linkend="LinkId"/></para>
  </refsection>

  <refsection>
    <title>Fields</title>

    <informaltable colsep="0" frame="topbot">
      <tgroup cols="2">
        <colspec colnum="1" colwidth=".5*"/>

        <colspec colnum="2" colwidth="3*"/>

        <thead>
          <row>
            <entry>Field</entry>

            <entry>Description</entry>
          </row>
        </thead>

        <tbody>
          <row>
            <entry><guilabel>FieldName</guilabel></entry>

            <entry><para>Description of the field. Use special formatting for
            <guilabel>command names</guilabel> and <guilabel>field
            names</guilabel>. See the next cell for an
            example.</para><variablelist>
                <varlistentry>
                  <term>Data Type</term>

                  <listitem>
                    <para>The data type from the Data Type column of the Field
                    Spec</para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Allowed Values</term>

                  <listitem>
                    <para>a range spec of list of allowed values from the
                    Range column of the Field Spec</para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Access</term>

                  <listitem>
                    <para>set, get, or only one (from the Access column of the
                    Field Spec)</para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Default Value</term>

                  <listitem>
                    <para>value from the Default Value column of the Field
                    Spec</para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Interfaces</term>

                  <listitem>
                    <para>GUI, script, or only one</para>
                  </listitem>
                </varlistentry>
              </variablelist></entry>
          </row>

          <row>
            <entry><guilabel>Epoch</guilabel></entry>

            <entry><para>The time and date corresponding to the specified
            orbit state. It defines the start time for propagation using the
            <guilabel>Propagate</guilabel> command.</para><variablelist>
                <varlistentry>
                  <term>Data Type</term>

                  <listitem>
                    <para>Time</para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Allowed Values</term>

                  <listitem>
                    <para>Gregorian: <userinput>04 Oct 1957
                    12:00:00.000</userinput> &lt;= <guilabel>Epoch</guilabel>
                    &lt;= <userinput>28 Feb 2100
                    00:00:00.000</userinput></para>

                    <para>Modified Julian: <userinput>6116.0</userinput> &lt;=
                    <guilabel>Epoch</guilabel> &lt;=
                    <userinput>58127.5</userinput></para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Default Value</term>

                  <listitem>
                    <para>21545</para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Required</term>

                  <listitem>
                    <para>no</para>
                  </listitem>
                </varlistentry>

                <varlistentry>
                  <term>Interfaces</term>

                  <listitem>
                    <para>GUI, script</para>
                  </listitem>
                </varlistentry>
              </variablelist></entry>
          </row>
        </tbody>
      </tgroup>
    </informaltable>
  </refsection>

