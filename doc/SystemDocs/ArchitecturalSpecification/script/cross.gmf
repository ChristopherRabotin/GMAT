function [vec3] = cross(vec1, vec2)

% This function takes two 3-vectors in a GMAT array and
% constructs their cross product
Create Array vec3[3,1]

vec3(1,1) = vec1(2,1) * vec2(3,1) - vec1(3,1) * vec2(2,1);
vec3(2,1) = vec1(3,1) * vec2(1,1) - vec1(1,1) * vec2(3,1);
vec3(3,1) = vec1(1,1) * vec2(2,1) - vec1(2,1) * vec2(1,1);

